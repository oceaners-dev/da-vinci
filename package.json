{
  "name": "da-vinci-ui",
  "description": "Next.js UI kit written with TailwindCSS and TypeScript.",
  "version": "0.2.1-14",
  "author": "oceaners-dev",
  "license": "ISC",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "dev": "concurrently \"pnpm build\" \"pnpm storybook\"",
    "build": "NODE_ENV=production rm -rf ./dist && rollup -c && cp ./dist/styles.css ./example/components/da-vinci-ui/styles.css",
    "watch": "concurrently \"pnpm watch:rollup\"",
    "watch:rollup": "NODE_ENV=production rollup -c -w",
    "make:docs": "typedoc",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"{src,__tests__}/**/*.+(ts|js|tsx)\"",
    "lint:fix": "pnpm lint --fix",
    "test": "jest --coverage",
    "test:ci": "pnpm test --ci",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "commit": "cz",
    "storybook": "start-storybook -p 6006 --quiet",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "release": "pnpm release-it",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "lint-staged": {
    "./{src,__tests__}/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true,
      "access": "public",
      "publishPath": "--registry https://registry.npmjs.org"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@ryansonshine/commitizen": "4.2.8",
    "@ryansonshine/cz-conventional-changelog": "3.3.4",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-interactions": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@storybook/react": "6.5.13",
    "@storybook/storybook-deployer": "2.8.16",
    "@storybook/testing-library": "0.0.13",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.7",
    "@types/react-test-renderer": "18.0.0",
    "@types/react-transition-group": "4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "8.2.5",
    "concurrently": "7.4.0",
    "cookie-universal": "^2.2.2",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.7",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "fs-extra": "^10.1.0",
    "husky": "8.0.1",
    "inspx": "0.0.1-alpha.6",
    "jest": "29.1.2",
    "lint-staged": "13.0.3",
    "next": "13.0.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "purgecss": "^5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "release-it": "15.5.0",
    "tailwindcss": "^3.2.1",
    "ts-jest": "29.0.3",
    "tsup": "6.2.3",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "chroma-js": ">=2",
    "classnames": ">=2",
    "cookie-universal": ">=2",
    "next": ">=13",
    "prism-react-renderer": ">=1",
    "react": ">=18",
    "react-aria": ">=3",
    "react-dom": ">=18",
    "react-uuid": ">=2",
    "tailwindcss": "3.2.4",
    "zustand": ">=4"
  },
  "resolutions": {
    "glob-parent": ">=5.1.2",
    "parse-path": ">5.0.0",
    "parse-url": ">=8.1.0",
    "terser": ">=5.14.2",
    "trim": ">=0.0.3",
    "trim-newlines": ">=3.0.1"
  },
  "dependencies": {
    "@faker-js/faker": "7.6.0",
    "@jest/globals": "29.3.1",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "9.0.2",
    "@storybook/addon-postcss": "2.0.0",
    "@testing-library/dom": "8.19.0",
    "@testing-library/react": "13.4.0",
    "@types/chroma-js": "2.1.4",
    "@types/enzyme": "3.10.12",
    "@wojtekmaj/enzyme-adapter-react-17": "0.8.0",
    "chroma-js": "2.4.2",
    "classnames": "2.3.2",
    "enzyme": "3.11.0",
    "jest-environment-jsdom": "29.3.1",
    "prism-react-renderer": "1.3.5",
    "react-aria": "3.21.0",
    "react-uuid": "2.0.0",
    "rollup": "^2.79.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-dts": "5.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-tailwind": "1.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "tailwindcss-classnames": "3.0.7",
    "tslib": "2.4.1",
    "typedoc": "0.23.21",
    "zustand": "4.1.4"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/styles.css"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "directories": {
    "example": "example",
    "test": "__tests__"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.jest.json"
        }
      ]
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ]
  }
}
